source [find board/ti_cc26x0_launchpad.cfg]

init;
halt;

flash protect 0 0 last off;
flash write_image erase target/thumbv7em-none-eabi/release/launchxlccfg-cc13x0.bin 0x1FFA8 bin;
verify_image_checksum target/thumbv7em-none-eabi/release/launchxlccfg-cc13x0.bin 0x1FFA8 bin;
soft_reset_halt
resume;
exit;
